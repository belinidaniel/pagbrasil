<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>QuoteCanceled</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(Status),   ISPICKVAL(PRIORVALUE(Status), &apos;Canceled&apos;)  )</errorConditionFormula>
    <errorDisplayField>Status</errorDisplayField>
    <errorMessage>Não pode alterar o status de cotação cancelada</errorMessage>
</ValidationRule>
