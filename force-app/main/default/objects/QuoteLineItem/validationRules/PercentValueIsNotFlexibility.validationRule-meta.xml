<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PercentValueIsNotFlexibility</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(PercentValue__c), Flexibilization__c = &apos;Sem Autonomia&apos;)</errorConditionFormula>
    <errorDisplayField>PercentValue__c</errorDisplayField>
    <errorMessage>Valor não tem autonomia para alteração</errorMessage>
</ValidationRule>
