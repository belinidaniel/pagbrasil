<apex:page pageStyle=""
           showHeader="false" 
           applyHtmlTag="false" 
           applyBodyTag="false" 
           apiVersion="28.0" 
           showQuickActionVfHeader="false"
           renderAs="pdf"
           standardController="Opportunity"
           extensions="ContractController"
           >
    <html>
        <head>
            <style>
                * {
                    font-family: Arial, Arial, sans-serif !important;
                    font-size: 14px;
                }

                @page {
                    size: A4;
                    margin-top: 3cm;
                    margin-bottom: 3cm;
                    margin-left: 2.75cm;
                    margin-right: 2cm;
                }
                
                table {
                    width: 100%;
                    border-collapse: collapse;
                }
                
                table td {
                    text-align: center;
                } 
                
                table th {
                    text-align: center;
                } 
                
                td, th {
                    border-bottom: 1px solid black;
                    padding: 5px;
                    text-align: left;
                }
                
                .body {
                    white-space: pre;
                }
                
                .box-assinatura {
                    text-align: center;
                    display: flex; 
                    flex-direction: column; 
                    align-items: center; 
                    gap: 10px;
                }
                .assinatura {
                    display: inline-block;
                    text-align: left;
                    font-size: 16px;
                    font-style: italic;
                    width: 82%;
                    border-bottom: 1px #000 solid;
                    padding-bottom: 10px;
                }
                .nomes-assinatura {
                    display: inline-block;
                    text-align: left;
                    font-size: 11px;
                    /* line-height: 11px; */
                    width: 82%;
                    padding-top: 10px;
                }
                
                .page-break {
                    page-break-before: always;
                }
                .no-margin{
                    margin: 0rem;
                    page-break-inside: avoid;
                }
                
                .group {
                    min-height: 30px;
                    margin: 0px;
                    line-height: 1.3;
                }
                
            </style>
        </head>

        <body>

            <apex:variable var="startNumber" value="2"/>

            <img src="{!URLFOR($Resource.Logo)}" style="width: 25%; height: 25%;margin-top: -5%"/>
            
            <h2 style="text-align: center; padding-bottom: 80px;">AGREEMENT</h2>

            <apex:form style="margin-top: -10%;">
                <table style="width: 100%; border-collapse: collapse;">
                    <tr>
                        <th style="border-right: 1px solid black; width: 30%;text-align:left;">Merchant: </th>
                        <th style="font-weight: normal;text-align: left!important;"> {!opportunity.Account.CNPJ__c} - {!legalName}</th>
                    </tr>
                    <tr>
