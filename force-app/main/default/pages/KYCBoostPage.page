<apex:page controller="KYCBoostController">
	<head>
		<style>
				* {
					font-family: Arial, Arial, sans-serif !important;
					font-size: 14px;
				}

				body {
					display: flex;
					flex-direction: column;
					justify-content: center;
					width: 80vw;
				}

				.logo {
					align-self: center;
				}

				.main-btn {
					color: #fff;
					background: #007bff;
					border-color: #007bff;
					border: 1px solid transparent;
					padding: .375rem .75rem;
					font-size: 1rem;
					line-height: 1.5;
					border-radius: .25rem;
					width: max-content;
					align-self: end;
					cursor: pointer;
				}

				.form-title {
					margin-top: 16px;
					font-weight: 700;
					font-size: 14px;
					align-self: center;
				}

				h3 {
					display: flex;
					box-sizing: border-box;
					color: #878685;
					padding: 8px 8px 2px 8px;
					font-size: 12px;
					font-weight: 700;
				}

				.title-container {
					display: flex;
					box-sizing: border-box;
					border-radius: 4px;
					padding: 4px;
					height: 36px;
					align-items: center;
					margin-top: 8px;
					margin-bottom: 8px;
				}

				.title {
					font-size: 1rem;
					color: black;
					font-weight: 600;
				}

				.data-container {
					display: grid;
					grid-template-columns: 0.9fr 0.9fr;
					gap: 4px;
					transform: translateX(8px);
				}

				.three-part-container {
					display: grid;
					grid-template-columns: 0.9fr 0.9fr 0.9fr;
					gap: 4px;
					transform: translateX(8px);
				}

				.activity-container {
					display: grid;
					grid-template-columns: 0.4fr 2.0fr 0.4fr;
					gap: 4px;
					transform: translateX(8px);
				}

				.value {
					font-weight: 400;
					margin: 4px 0px 12px 2px;
				}

				.value-url {
					line-break: anywhere;
					font-weight: 400;
					margin: 4px 0px 12px 2px;
				}

				.contact-container {
					display: flex;
					flex-direction: column;
					box-sizing: border-box;
					padding: 8px 12px;
					border-radius: 4px;
					border: solid #ededed 1px;
					margin-top: 2px;
					margin-bottom: 8px;
					break-inside: avoid;
				}

				.small-title {
					display: flex;
					box-sizing: border-box;
					color: #878685;
					padding: 8px 8px 2px 8px;
					font-size: 11px;
					font-weight: 500;
					margin: 0px;
				}

				.file-links {
					transform: translateX(16px);
				}

				.no-break {
					break-inside: avoid;
				}

				.break-after {
					break-after: page;
				}

				@media (min-width: 1440px) {
					body {
						width: 60vw;
					}
				}

				@media (max-width: 425px) {
					.data-container {
						display: grid;
						grid-template-columns: 1fr;
						gap: 4px;
					}
				}

				@media print {
					.main-btn {
						display: none;
					}
				}
		</style>
	</head>
	<body>
		<button class="main-btn" onclick="window.print()">
			Gerar PDF
		</button>
		<div class="logo">
