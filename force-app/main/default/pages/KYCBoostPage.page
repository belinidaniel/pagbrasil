		<h1 class="form-title">RELATÓRIO DE EMPRESA BIGBOOST</h1>
        <main>
			<apex:outputPanel rendered="{!hasBasicBB}">
			    <section class="section">
                    <header class="title-container">
                        <h2 class="title">Dados Básicos</h2>
                    </header>
                    <article class="data-container">
                        <div class="field">
                            <strong class="label">CNPJ:</strong>
                            <p class="value"><apex:outputText value="{!taxIdNumber}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">País:</strong>
                            <p class="value"><apex:outputText value="{!taxIdCountry}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Natureza legal:</strong>
                            <p class="value"><apex:outputText value="{!legalNatureActivity}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Status:</strong>
                            <p class="value"><apex:outputText value="{!taxIdStatus}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Nome Oficial:</strong>
                            <p class="value"><apex:outputText value="{!officialName}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Nome Fantasia:</strong>
                            <p class="value"><apex:outputText value="{!tradeName}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Data de fundação:</strong>
                            <p class="value"><apex:outputText value="{!foundedDate}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Regime tributário:</strong>
                            <p class="value"><apex:outputText value="{!taxRegime}"/></p>
                        </div>
                    </article>
                </section>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!hasActivitiesBB}">
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">Atividades</h2>
                    </header>
                   <apex:repeat value="{!IF(NOT(ISBLANK(basicBB['Result'])), basicBB['Result'][0]['BasicData']['Activities'], null)}" var="activity">
                        <apex:outputPanel rendered="{!NOT(ISNULL(activity))}">
                            <span class="contact-container">
                                <article class="activity-container">
                                    <div class="field">
                                        <strong class="label">Código:</strong>
                                        <p class="value"><apex:outputText value="{!activity['Code']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Atividade:</strong>
                                        <p class="value"><apex:outputText value="{!activity['Activity']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Principal?</strong>
                                        <p class="value"><apex:outputText value="{!IF(activity['IsMain'], 'Sim', 'Não')}"/></p>
                                    </div>
                                </article>
                            </span>
                        </apex:outputPanel>
                    </apex:repeat>
                </section>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!hasAddressesBB}">
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">Endereços</h2>
                    </header>
                     <apex:repeat value="{!IF(NOT(ISBLANK(addressesBB['Result'])), addressesBB['Result'][0]['ExtendedAddresses']['Addresses'], null)}" var="address">
                        <apex:outputPanel rendered="{!NOT(ISNULL(address))}">
                            <span class="contact-container">
                                <article class="data-container">
                                    <div class="field">
                                        <strong class="label">Endereço:</strong>
                                        <p class="value"><apex:outputText value="{!address['AddressMain']}, {!address['Number']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Complemento:</strong>
                                        <p class="value"><apex:outputText value="{!address['Complement']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">UF:</strong>
                                        <p class="value"><apex:outputText value="{!address['State']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Cidade:</strong>
                                        <p class="value"><apex:outputText value="{!address['City']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Bairro:</strong>
                                        <p class="value"><apex:outputText value="{!address['Neighborhood']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">CEP:</strong>
                                        <p class="value"><apex:outputText value="{!address['ZipCode']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Endereço principal?</strong>
                                        <p class="value"><apex:outputText value="{!IF(address['IsMainForEntity'], 'Sim', 'Não')}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Está ativo?</strong>
                                        <p class="value"><apex:outputText value="{!IF(address['IsActive'], 'Sim', 'Não')}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Endereço recente?</strong>
                                        <p class="value"><apex:outputText value="{!IF(address['IsRecentForEntity'], 'Sim', 'Não')}"/></p>
                                    </div>
                                </article>
                            </span>
                        </apex:outputPanel>
                    </apex:repeat>
                </section>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!hasPhonesBB}">
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">Telefones</h2>
                    </header>
                    <apex:repeat value="{!IF(NOT(ISBLANK(phonesBB['Result'])), phonesBB['Result'][0]['ExtendedPhones']['Phones'], null)}" var="phone">
                        <apex:outputPanel rendered="{!NOT(ISNULL(phone))}">
                            <span class="contact-container">
                                <article class="data-container">
                                    <div class="field">
                                        <strong class="label">Telefone:</strong>
                                        <p class="value">+ <apex:outputText value="{!phone['CountryCode']}"/> (<apex:outputText value="{!phone['AreaCode']}"/>) <apex:outputText value="{!phone['Number']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Tipo:</strong>
                                        <p class="value"><apex:outputText value="{!phone['Type']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Telefone principal?</strong>
                                        <p class="value"><apex:outputText value="{!IF(phone['IsMainForEntity'], 'Sim', 'Não')}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Está ativo?</strong>
                                        <p class="value"><apex:outputText value="{!IF(phone['IsActive'], 'Sim', 'Não')}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">É recente?</strong>
                                        <p class="value"><apex:outputText value="{!IF(phone['IsRecentForEntity'], 'Sim', 'Não')}"/></p>
                                    </div>
                                </article>
                            </span>
                        </apex:outputPanel>
                    </apex:repeat>
                </section>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!hasEmailsBB}">
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">E-mails</h2>
                    </header>
                    <apex:repeat value="{!emailsBB}" var="email">
                        <span class="contact-container">
                            <article class="data-container">
                                <div class="field">
                                    <strong class="label">Email:</strong>
                                    <p class="value"><apex:outputText value="{!email['EmailAddress']}"/></p>
                                </div>
                                <div class="field">
                                    <strong class="label">Email principal?</strong>
                                    <p class="value"><apex:outputText value="{!IF(email['IsMainForEntity'], 'Sim', 'Não')}"/></p>
                                </div>
                                <div class="field">
                                    <strong class="label">Está ativo?</strong>
                                    <p class="value"><apex:outputText value="{!IF(email['IsActive'], 'Sim', 'Não')}"/></p>
                                </div>
                                <div class="field">
                                    <strong class="label">É recente?</strong>
                                    <p class="value"><apex:outputText value="{!IF(email['IsRecentForEntity'], 'Sim', 'Não')}"/></p>
                                </div>
                            </article>
                        </span>
                    </apex:repeat>
                </section>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!hasDomainsBB}">
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">Domínios</h2>
                    </header>
                    <apex:repeat value="{!IF(NOT(ISBLANK(domainsBB['Result'])), domainsBB['Result'][0]['Domains'], null)}" var="domain">
                        <apex:outputPanel rendered="{!NOT(ISNULL(domain))}">
                            <span class="contact-container">
                                <article class="data-container">
                                    <div class="field">
                                        <strong class="label">Host:</strong>
                                        <p class="value"><apex:outputText value="{!domain['HostName']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Nome:</strong>
                                        <p class="value"><apex:outputText value="{!domain['DomainName']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Classificação:</strong>
                                        <p class="value"><apex:outputText value="{!domain['DomainClass']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Segmento:</strong>
                                        <p class="value"><apex:outputText value="{!domain['DomainMainSegment']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Local de hospedagem:</strong>
                                        <p class="value"><apex:outputText value="{!domain['IspRegion']}, {!domain['IspCountry']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Está ativo?</strong>
                                        <p class="value"><apex:outputText value="{!IF(domain['IsActive'], 'Sim', 'Não')}"/></p>
                                    </div>
                                </article>
                            </span>
                        </apex:outputPanel>
                    </apex:repeat>
                </section>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!hasOwnersBB}">
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">KYC e Compliance</h2>
                    </header>
                    <article class="three-part-container">
                        <div class="field">
                            <strong class="label">Politicamente exposta: </strong>
                            <p class="value"><apex:outputText value="{!IF(NOT(ISBLANK(kycBB['Result'])), IF(kycBB['Result'][0]['KycData']['IsCurrentlyPEP'], 'Sim', 'Não'), '')}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Sancionada atualmente: </strong>
                            <p class="value"><apex:outputText value="{!IF(NOT(ISBLANK(kycBB['Result'])), IF(kycBB['Result'][0]['KycData']['IsCurrentlySanctioned'], 'Sim', 'Não'), '')}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Doadora eleitoral recente: </strong>
                            <p class="value"><apex:outputText value="{!IF(NOT(ISBLANK(kycBB['Result'])), IF(kycBB['Result'][0]['KycData']['IsCurrentlyElectoralDonor'], 'Sim', 'Não'), '')}"/></p>
                        </div>
                    </article>
                </section>
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">KYC e Compliance dos Sócios</h2>
                    </header>
                    <apex:outputPanel rendered="{!hasOwnersBB}">
                        <apex:repeat value="{!ownersList}" var="owner">
                            <span class="contact-container">
                                <article class="data-container">
                                    <div class="field">
                                        <strong class="label">Documento do Sócio:</strong>
                                        <p class="value"><apex:outputText value="{!owner['TaxId']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Politicamente exposta:</strong>
                                        <p class="value"><apex:outputText value="{!IF(owner['IsCurrentlyPEP'], 'Sim', 'Não')}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Sancionada atualmente:</strong>
                                        <p class="value"><apex:outputText value="{!IF(owner['IsCurrentlySanctioned'], 'Sim', 'Não')}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Doadora eleitoral recente:</strong>
                                        <p class="value"><apex:outputText value="{!IF(owner['IsCurrentlyElectoralDonor'], 'Sim', 'Não')}"/></p>
                                    </div>
                                </article>
                            </span>
                        </apex:repeat>
                    </apex:outputPanel>
                </section>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!hasAppsBB}">
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">Presença em Apps e Plataformas</h2>
                    </header>
					<article class="data-container">
						<div class="field">
							<strong class="label">Total de redes sociais: </strong>
							<p class="value"><apex:outputText value="{!appsSummary.totalSocialNetworks}"/></p>
						</div>
						<div class="field">
							<strong class="label">Total de resultados de buscas: </strong>
							<p class="value"><apex:outputText value="{!appsSummary.totalSearchResults}"/></p>
						</div>
						<div class="field">
							<strong class="label">Total de posts em mídias sociais: </strong>
							<p class="value"><apex:outputText value="{!appsSummary.totalSocialMediaPosts}"/></p>
						</div>
						<div class="field">
							<strong class="label">Percentual nas mídias sociais de seguidores/seguindo: </strong>
							<p class="value"><apex:outputText value="{!appsSummary.followerToFollowingRatio}"/></p>
						</div>
						<div class="field">
							<strong class="label">Possui Facebook? </strong>
							<p class="value"><apex:outputText value="{!IF(appsSummary.hasFacebook, 'Sim', 'Não')}"/></p>
						</div>
						<div class="field">
							<strong class="label">Possui Twitter? </strong>
							<p class="value"><apex:outputText value="{!IF(appsSummary.hasTwitter, 'Sim', 'Não')}"/></p>
						</div>
						<div class="field">
							<strong class="label">Possui LinkedIn? </strong>
							<p class="value"><apex:outputText value="{!IF(appsSummary.hasLinkedIn, 'Sim', 'Não')}"/></p>
						</div>
						<div class="field">
							<strong class="label">Possui Instagram? </strong>
							<p class="value"><apex:outputText value="{!IF(appsSummary.hasInstagram, 'Sim', 'Não')}"/></p>
						</div>
						<div class="field">
							<strong class="label">Possui OLX? </strong>
							<p class="value"><apex:outputText value="{!IF(appsSummary.hasOLX, 'Sim', 'Não')}"/></p>
						</div>
						<div class="field">
							<strong class="label">Possui MercadoLivre? </strong>
							<p class="value"><apex:outputText value="{!IF(appsSummary.hasMercadoLivre, 'Sim', 'Não')}"/></p>
						</div>
					</article>
                </section>
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">Perfis</h2>
                    </header>
                    <apex:repeat value="{!appProfiles}" var="app">
                        <apex:outputPanel rendered="{!NOT(ISNULL(app))}">
                            <span class="contact-container">
                                <article class="data-container">
                                    <div class="field">
                                        <strong class="label">Nome do App:</strong>
                                        <p class="value"><apex:outputText value="{!app['AppName']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">URL:</strong>
                                        <p class="value"><apex:outputText value="{!app['ProfileUrl']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">MatchRate de nome de perfil:</strong>
                                        <p class="value"><apex:outputText value="{!app['DisplayNameMatchRate']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">MatchRate de nome de usuário:</strong>
                                        <p class="value"><apex:outputText value="{!app['UserNameMatchRate']}"/></p>
                                    </div>
                                </article>
                            </span>
                        </apex:outputPanel>
                    </apex:repeat>
                </section>
                <section>
                    <header class="title-container">
                        <h2 class="title">Resultados de busca</h2>
                    </header>
                     <apex:repeat value="{!searchEngineResults}" var="engine">
                        <apex:outputPanel rendered="{!NOT(ISNULL(engine))}">
                            <span class="contact-container">
                                <article class="data-container">
                                    <div class="field">
                                        <strong class="label">Motor de busca:</strong>
                                        <p class="value"><apex:outputText value="{!engine['SearchEngineName']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Título da página:</strong>
                                        <p class="value"><apex:outputText value="{!engine['PageTitle']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Descrição da página:</strong>
                                        <p class="value"><apex:outputText value="{!engine['PageDescription']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">URL da página:</strong>
                                        <p class="value-url"><apex:outputText value="{!engine['PageUrl']}"/></p>
                                    </div>
                                </article>
                            </span>
                        </apex:outputPanel>
                    </apex:repeat>
                </section>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!hasMediaBB}">
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">Exposição e Perfil na Mídia</h2>
                    </header>
                    <article class="three-part-container">
                        <div class="field">
                            <strong class="label">Nível de celebridade:</strong>
                            <p class="value"><apex:outputText value="{!celebrityLevel}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Nível de explosição:</strong>
                            <p class="value"><apex:outputText value="{!mediaExposureLevel}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Nível de impopularidade:</strong>
                            <p class="value"><apex:outputText value="{!unpopularityLevel}"/></p>
                        </div>
					</article>
				</section>
			</apex:outputPanel>
			<apex:outputPanel rendered="{!hasProcessesBB}">
				<section class="section">
					<header class="title-container">
						<h2 class="title">Ações Judiciais</h2>
					</header>
					<article class="data-container">
            			<div class="field">
						<strong class="label">Total de processos:</strong>
							<p class="value">
								<apex:outputText value="{!lawsuits['TotalLawsuits']}"/>
							</p>
            			</div>
            			<div class="field">
							<strong class="label">Processos como autor:</strong>
							<p class="value">
								<apex:outputText value="{!lawsuits['TotalLawsuitsAsAuthor']}"/>
							</p>
            			</div>
            			<div class="field">
							<strong class="label">Processos como réu:</strong>
							<p class="value">
								<apex:outputText value="{!lawsuits['TotalLawsuitsAsDefendant']}"/>
							</p>
						</div>
						<div class="field">
							<strong class="label">Data do último processo:</strong>
							<p class="value">
								<apex:outputText value="{!LastLawsuitDate}"/>
							</p>
						</div>
						<div class="field">
							<strong class="label">Data do primeiro processo:</strong>
							<p class="value">
								<apex:outputText value="{!FirstLawsuitDate}"/>
							</p>
						</div>
					</article>
					<apex:repeat value="{!processesBB}" var="process">
						<apex:outputPanel rendered="{!NOT(ISNULL(process))}">
							<span class="contact-container">
								<article class="data-container">
									<div class="field">
										<strong class="label">Número:</strong>
										<p class="value"><apex:outputText value="{!process['Number']}"/></p>
									</div>
									<div class="field">
										<strong class="label">Tipo:</strong>
										<p class="value"><apex:outputText value="{!process['Type']}"/></p>
									</div>
									<div class="field">
										<strong class="label">Assunto:</strong>
										<p class="value"><apex:outputText value="{!process['MainSubject']}"/></p>
									</div>
									<div class="field">
										<strong class="label">Status:</strong>
										<p class="value"><apex:outputText value="{!process['Status']}"/></p>
									</div>
									<div class="field">
										<strong class="label">Data de publicação:</strong>
										<p class="value"><apex:outputText value="{!process['PublicationDate']}"/></p>
									</div>
								</article>
							</span>
						</apex:outputPanel>
					</apex:repeat>
				</section>
			</apex:outputPanel>
            <apex:outputPanel rendered="{!hasActivitiesBB}">
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">Indicadores de Atividade</h2>
                    </header>
                    <article class="data-container">
                        <div class="field">
                            <strong class="label">Nível de atividade:</strong>
                            <p class="value"><apex:outputText value="{!activityBB['Result'][0]['ActivityIndicators']['ActivityLevel']}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Quantidade de funcionários:</strong>
                            <p class="value"><apex:outputText value="{!activityBB['Result'][0]['ActivityIndicators']['EmployeesRange']}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Estimativa de faturamento anual:</strong>
                            <p class="value"><apex:outputText value="{!activityBB['Result'][0]['ActivityIndicators']['IncomeRange']}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Empresa Ativa:</strong>
                            <p class="value"><apex:outputText value="{!IF(activityBB['Result'][0]['ActivityIndicators']['HasActivity'], 'Sim', 'Não')}"/></p>
                        </div>
                        <div class="field">
                            <strong class="label">Número de filiais:</strong>
                            <p class="value"><apex:outputText value="{!activityBB['Result'][0]['ActivityIndicators']['NumberOfBranches']}"/></p>
                        </div>
                    </article>
                </section>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!hasRelationshipsBB}">
                <section class="section">
                    <header class="title-container">
                        <h2 class="title">Relacionamentos</h2>
                    </header>
                    <apex:repeat value="{!IF(NOT(ISBLANK(relationshipsBB['Result'])), relationshipsBB['Result'][0]['Relationships']['Relationships'], null)}" var="relationship">
                         <apex:outputPanel rendered="{!NOT(ISNULL(relationship))}">
                            <span class="contact-container">
                                <article class="data-container">
                                    <div class="field">
                                        <strong class="label">Entidade relacionada:</strong>
                                        <p class="value"><apex:outputText value="{!relationship['RelatedEntityName']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Tipo de documento:</strong>
                                        <p class="value"><apex:outputText value="{!relationship['RelatedEntityTaxIdType']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Número do documento:</strong>
                                        <p class="value"><apex:outputText value="{!relationship['RelatedEntityTaxIdNumber']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Nível do relacionamento:</strong>
                                        <p class="value"><apex:outputText value="{!relationship['RelationshipLevel']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Nome do relacionamento:</strong>
                                        <p class="value"><apex:outputText value="{!relationship['RelationshipName']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Tipo do relacionamento:</strong>
                                        <p class="value"><apex:outputText value="{!relationship['RelationshipType']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Data de início:</strong>
                                        <p class="value"><apex:outputText value="{!relationship['RelationshipStartDate']}"/></p>
                                    </div>
                                    <div class="field">
                                        <strong class="label">Data de término:</strong>
                                        <p class="value"><apex:outputText value="{!relationship['RelationshipEndDate']}"/></p>
                                    </div>
                                </article>
                            </span>
                        </apex:outputPanel>
                    </apex:repeat>
                </section>
            </apex:outputPanel>
        </main>
    </body>
</apex:page>