<html>
    <head>
        <style>
            @page {
                margin: 0;
                padding: 0;
                size: 1143px 644px;
                border: none;
            }

            @font-face {
                font-family: 'Nunito';
                src: url('{!URLFOR($Resource.NunitoRegular)}') format('truetype');
            }

            @font-face {
                font-family: 'NunitoLight';
                src: url('{!URLFOR($Resource.NunitoLight)}') format('truetype');
            }

            @font-face {
                font-family: 'MontserratBold';
                src: url('{!URLFOR($Resource.MontserratBold)}') format('truetype');
            }
            
            body {
                margin: 0;
                padding: 0;
                font-size: 12px;
                font-family: 'NunitoLight', sans-serif;
            }

            .page {
                position: relative;
                width: 1143px;
                height: 644px;
            }

            .background-image {
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
            }

            .row {
                display: block;
            }

            .col {
                float: left;
            }

            .clearfix {
                clear: both;
            }

            .background-color-blue-100 {
                position: absolute;
                top: 0;
                left: 0;
                background-color: #0058B5;
                width: 1143px;
                height: 644px;
            }

            .background-color-white-100 {
                position: absolute;
                top: 0;
                left: 0;
                z-index: 0;
                background-color: white;
                width: 100%;
                height: 100%;
            }

            .background-color-white {
                position: absolute;
                background-color: white;  
                z-index: 1;
                top: 0;
                right: 0;
                height: 100%;
            }

            .background-color-blue {
                position: absolute;
                top: 0;
                left: 0;
                z-index: 0;
                background-color: #0058B5;
                width: 100%;
                height: 100%;
            }

            .background-color-grey {
                position: absolute;
                background-color: #F6F6F6;  
                z-index: 1;
                top: 0;
                right: 0;
                height: 100%;
            }

            .page-content {
                position: absolute;
                z-index: 2;
                color: black;
            }

            .titulo{
                position: absolute;
                top: 265px;
                left: 713px;
                width: 400px; /* Define a largura da div */
                height: 300px;
                padding: 1px; /* Adiciona um espaçamento interno */
                overflow: hidden; /* Esconde o texto que ultrapassar a largura */
                text-overflow: ellipsis; /* Adiciona "..." ao final do texto que ultrapassar */
            }

            .cor{
                color: #828181;
            }

            .cor-black{
                color: #000000;
            }

            .cor-alternative{
                color: #3691F8;
                font-weight: bold;
            }
            
            .additional-terms-item {
                margin-top: 10px;
                line-height: 3px;
            }

            .additional-terms-item::before {
                font-size: 15px;
            }

            .terms-list {
                position: absolute;
                top: 200px;
                left: 0;
                width: 454px;
                padding-left: 50px;
                padding-right: 50px;
                color: #505050;
                font-size: 12px;
                line-height: 1.2;
            }

            .terms-list li {
                margin-top: 15px;
            }

            .terms-sublist li::before {
                top: -7px;
            }

            .terms-sublist>li:first-child {
                margin-top: 15px
            }

            ul {
                list-style: none;
                margin: 0;
                padding: 0;
            }

            li:first-child {
                margin-top: 0;
            }

            li {
                position: relative; /* Necessário para posicionar o marcador */
                margin-top: 40px;
                padding-left: 30px;
            }
            
            .shopify-list>li {
                margin-top: 30px;
            }

            li::before {
                content: "•";
                font-size: 21px;
                color: rgb(83, 156, 220);
                position: absolute;
                left: 0;
                top: 0;
            }
            
            .settlement-elegibility-title {
                font-weight: bold;
                margin-bottom: 15px;
            }

            .settlement-elegibility-item {
                overflow: hidden;
            }

            .settlement-elegibility-label {
                float: left
            }

            .settlement-elegibility-value {
                float: right;
            }

            .payout-solution-list>li:first-child {
                margin-top: 0;
            }

            .payout-solution-list>li {

                position: relative; /* Necessário para posicionar o marcador */
                margin-top: 20px;
                padding-left: 30px;
            }  

            .settlement-elegibility-container {
                position: absolute;
                top: 260px;
                left: 580px;
                width: 400px;
                padding: 10px 20px 10px 20px;
                background-color: #f9f9f9;
                font-size: 14px;
            }

            .payout-supplemental-terms-container {
                position: absolute;
                top: 170px;
                left: 580px;
                width: 440px;
                padding: 40px 20px 40px 20px;
                background-color: #007bff;
                color: #f9f9f9;
                font-size: 18px;
            }

            .payout-supplemental-terms-item {
                
                border-bottom: 1px solid black;
                width: 80%;
                margin: auto;
            }
            
            .payout-supplemental-terms-title {
                font-weight: bold;
                margin-bottom: 15px;
                text-align: center;
                font-size: 20px;
            }

            .payout-supplemental-terms-label {
                
                font-weight: bold;
                text-align: left;
            }

            .payout-supplemental-terms-value {
                text-align: left;
            }
        </style>
    </head>
