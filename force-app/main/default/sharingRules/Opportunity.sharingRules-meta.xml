    <sharingCriteriaRules>
        <fullName>Legal_Compliance_Role_Edit</fullName>
        <accessLevel>Edit</accessLevel>
        <label>Legal &amp; Compliance Role Edit</label>
        <sharedTo>
            <role>Legal_and_Compliance</role>
        </sharedTo>
        <criteriaItems>
            <field>StageName</field>
            <operation>equals</operation>
            <value>Compliance</value>
        </criteriaItems>
        <criteriaItems>
            <field>StageName</field>
            <operation>equals</operation>
            <value>Contract</value>
        </criteriaItems>
        <criteriaItems>
            <field>StageName</field>
            <operation>equals</operation>
            <value>Homologation</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>Legal_Compliance_Role_View</fullName>
        <accessLevel>Read</accessLevel>
        <label>Legal &amp; Compliance Role View</label>
        <sharedTo>
            <role>Legal_and_Compliance</role>
        </sharedTo>
        <booleanFilter>1 OR 2 OR 3 OR 4 OR 5</booleanFilter>
        <criteriaItems>
            <field>StageName</field>
            <operation>equals</operation>
            <value>Discovery</value>
        </criteriaItems>
        <criteriaItems>
            <field>StageName</field>
            <operation>equals</operation>
            <value>Negotiation</value>
        </criteriaItems>
        <criteriaItems>
            <field>StageName</field>
